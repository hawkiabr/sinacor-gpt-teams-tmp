{
    "type": "AdaptiveCard",
    "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
    "version": "1.5",
    "msteams": {
        "width": "Full"
    },
    "body": [
        {
            "type": "Image",
            "url": "data:image/png;base64,",
            "size": "large",
            "horizontalAlignment": "center"
        },
        {
            "type": "TextBlock",
            "text": "Converse sobre o Sinacor",
            "wrap": true,
            "style": "heading",
            "size": "ExtraLarge",
            "horizontalAlignment": "Center"
        },
        {
            "type": "TextBlock",
            "text": "Pergunte qualquer coisa ou tente um exemplo",
            "style": "heading",
            "horizontalAlignment": "Center",
            "wrap": true
        },
        {
            "type": "Container",
            "style": "emphasis",
            "items": [
                {
                    "type": "ActionSet",
                    "$data": "${questions}",
                    "actions": [
                        {
                            "type": "Action.Execute",
                            "title": "${$data}",
                            "tooltip": "${$data}",
                            "verb": "example",
                            "data": {
                                "displayText": "${$data}",
                                "text": "${$data}"
                            }
                        }
                    ]
                }
            ]
        }
    ]
}